  "bugs": {
    "url": "https://github.com/indutny/gradtype/issues"
  },
  "homepage": "https://github.com/indutny/gradtype#readme",
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.0",
    "mocha": "^5.0.5",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@types/debug": "0.0.30",
    "debug": "^3.1.0"
